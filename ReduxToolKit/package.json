{
  "name": "reduxtoolkit",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "cross-env jest --collectCoverage=false --forceExit",
    "test:no-coverage": "cross-env jest --collectCoverage=false", 
    "test:silent": "cross-env jest --collectCoverage=false --reporters=jest-silent-reporter",
    "test:failures": "cross-env jest --onlyFailures --collectCoverage=false",
    "test-watch": "cross-env jest --watch --collectCoverage=false",
    "update-snapshots": "cross-env jest --updateSnapshot",
    "jest-single": "jest --coverage=false"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.2",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "^29.5.12",
    "crypto": "^1.0.1",
    "enzyme": "^3.11.0",
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^29.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
